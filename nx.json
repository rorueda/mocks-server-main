{
  "targetDefaults": {
    "build": {
      "cache": true
    }
  },
  "extends": "@nx/workspace/presets/npm.json",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nx/workspace/tasks-runners/default",
      "options": {}
    }
  },
  "implicitDependencies": {
    "pnpm-workspace.yaml": "*",
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "nx.json": "*",
    "scripts/**/*.*": "*",
    ".github/workflows/*.*": "*",
    "README.md": [
      "main"
    ],
    "LICENSE": [
      "main"
    ],
    "NOTICE": [
      "main"
    ]
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ],
    "serve": [
      {
        "target": "build",
        "projects": "self"
      }
    ],
    "test:e2e": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ],
    "test:unit": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "defaultBase": "master"
}
